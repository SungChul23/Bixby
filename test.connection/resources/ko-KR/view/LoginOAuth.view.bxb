result-view {
  match: LoginResponse (this)

  message {
    template ("#{value(message)}") {  
      speech ("#{value(message)}")
    }
  }
  
  render {
    layout {
      section {
        content {
          paragraph {
            value ("#{value(message)}")
          }
          attribution-link {
            label ("로그인 페이지로 이동")  
            url ("https://jkah.shop:8443/bixby/login")  
          }
        }
      }
    }
  }
}
