result-view {
  match: LoginResponse

  message {
    template ("로그인이 필요합니다.") { 
      speech ("로그인이 필요합니다.") 
    }
  }

  render {
    layout {
      section {
        content {
          title-card {
            title-area {
              slot1 {
                paragraph {
                  style(Title_L)
                  value ("로그인이 필요합니다.")  
                }
              }
            }
            on-click {
              url (url)  
              external-link-badge {
                label ("로그인 페이지로 이동")
              }
            }
          }
        }
      }
    }
  }
  
}
