action (RunGroup) {
  type (Constructor)
  description ("사용자가 선택한 그룹을 실행하는 기능")
  collect {
    input (groupName) {
      type (viv.core.Text) //빅스비 캡슐 공식 출시 이후 추가된 그룹이 있어도 viv.core.Text를 통해 그룹 별 제어 가능
      min (Optional) 
      description ("사용자가 실행할 그룹 이름")
    }
    input (userSession) { 
      type (userSession)
      min (Optional)
      description ("AWS서버에서 받은 가공된 토큰이 들어있는 userSession")
    }
  }
  output (RunGroupResult)
}